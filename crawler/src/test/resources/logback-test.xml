<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss} [%level] %logger{1} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="com.jammking.webprobe.crawler" level="DEBUG" />

    <root level="WARN">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>